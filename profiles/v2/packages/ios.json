{
  "common": {
    "buildPath": "project://build",
    "outputName": "ios",
    "mainBundleCompressionType": "merge_dep",
    "platform": "ios"
  },
  "options": {
    "ios": {
      "packageName": "test",
      "renderBackEnd": {
        "metal": true
      },
      "skipUpdateXcodeProject": false,
      "orientation": {
        "portrait": false,
        "upsideDown": false,
        "landscapeRight": true,
        "landscapeLeft": true
      },
      "osTarget": {
        "iphoneos": true,
        "simulator": true
      },
      "targetVersion": "12.0",
      "__version__": "1.0.0"
    }
  }
}
